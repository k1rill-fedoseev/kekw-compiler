# ARITHMETIC TEST
(plus 1 2) # 3
(plus -1 2) # 1
(plus -1 -2) # -3
(plus 1.2 2.3) # 3.5

(minus 1 2) # -1
(minus -1 2) # -3
(minus -1 -2) # 1
(minus 1.2 2.3) # -1.1

(times 1 2) # 2
(times -1 2) # -2
(times -1 -2) # 2
(times 2 2) # 4
(times 1.2 2.3) # 2.76

(divide 1 2) # 0.5
(divide -1 2) # -0.5
(divide -1 -2) # 0.5
(divide 4 2) # 2
(divide 4.5 0.5) # 9

